{
  "name": "license-token-website",
  "version": "1.8.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "generate:articles": "node scripts/generateArticles.js",
    "generate:sitemap": "node scripts/generateSitemap.js",
    "build": "npm run generate:articles && npm run generate:sitemap && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run license-token-website:serve-ssr",
    "serve:ssr": "node dist/license-token-website/server/main.js",
    "build:ssr": "ng build && ng run license-token-website:server",
    "prerender": "ng run license-token-website:prerender",
    "buidandserve:ssr": "npm run build:ssr && npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.6",
    "@angular/cdk": "~19.1.4",
    "@angular/common": "^19.1.6",
    "@angular/compiler": "^19.1.6",
    "@angular/core": "^19.1.6",
    "@angular/forms": "^19.1.6",
    "@angular/material": "~19.1.4",
    "@angular/platform-browser": "^19.1.6",
    "@angular/platform-browser-dynamic": "^19.1.6",
    "@angular/platform-server": "^19.1.6",
    "@angular/router": "^19.1.6",
    "@angular/ssr": "^19.1.8",
    "cheerio": "^1.0.0",
    "express": "^4.18.2",
    "marked": "^15.0.7",
    "ngx-cookie-service": "^19.1.0",
    "ngx-markdown": "^19.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.8.1",
    "xml2js": "^0.6.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.7",
    "@angular/cli": "^19.1.7",
    "@angular/compiler-cli": "^19.1.6",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.6",
    "@types/node": "^18.18.0",
    "@types/xml2js": "^0.4.14",
    "browser-sync": "^3.0.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.7.3"
  }
}
